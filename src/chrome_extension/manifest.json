{
    "name": "EstateHistory - Hemnet real estate history with images and data",
    "version": "1.0",
    "description": "All saved listings will have all info and images after they are sold",
    "permissions": ["activeTab", "declarativeContent","storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon@16w.png",
            "32": "images/icon@32w.png",
            "48": "images/icon@48w.png",
            "128": "images/icon@128w.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["*://*.hemnet.se/bostad/*"],
            "run_at": "document_end",
            "js": ["contentScript.js"]
        }
    ],
    "options_page": "options.html",
    "icons": {
        "16": "images/icon@16w.png",
        "32": "images/icon@32w.png",
        "48": "images/icon@48w.png",
        "128": "images/icon@128w.png"
      },
    "manifest_version": 2
  }